/**
 * Not gate:
 * out = not in
 */

CHIP Not {
    IN in;
    OUT out;

    PARTS:
    // Put your code here:
    Nand(a=in, b=in, out=out); 
    /** ex. a=1 & b=1 will output 0 in the truth table of a nand gate
     *  also, for a=0 & b=0 -> 1
     *  this will display correct opposite values for the not gate
     */
}
